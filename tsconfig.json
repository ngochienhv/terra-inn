{
  "extends": "expo/tsconfig.base",
  "include": [
    "./**/*"
  ],
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/constants/*": [
        "constants/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/screens/*": [
        "screens/*"
      ],
      "@/redux/*": [
        "redux/*"
      ]
    }
  }
}